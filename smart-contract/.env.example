# Mantle Gas-Less Relayer - Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Deployment Configuration
PRIVATE_KEY=your_deployment_private_key_here
MANTLE_TESTNET_RPC_URL=https://rpc.testnet.mantle.xyz

# Optional: Relayer address to add during deployment
RELAYER_ADDRESS=

# Verification (for contract verification on block explorer)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Network Configuration
MANTLE_TESTNET_CHAIN_ID=5003

# Gas Configuration (optional)
GAS_PRICE=
GAS_LIMIT=
